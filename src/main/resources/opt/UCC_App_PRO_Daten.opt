        <term_definitions code="at0000">
          <items id="description">Die Frequenz und zugehörige Attribute für die Puls- oder Herzfrequenz.</items>
          <items id="text">Pulsfrequenz/Herzfrequenz</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Structure</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">@ internal @</items>
          <items id="text">History</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">Ein Standardwert, ein spezifizierter Zeitpunkt oder ein Intervallereignis, welches explizit in einem Template oder während der Laufzeit definiert werden kann.</items>
          <items id="text">Jedes Ereignis</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">Die Frequenz, gemessen in Schlägen pro Minute.</items>
          <items id="text">Frequenz</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">Regelmäßigkeit der Puls-/Herzfrequenz.</items>
          <items id="text">Regelmäßigkeit</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Der Verlauf ist regelmäßig.</items>
          <items id="text">Regelmäßig</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">Der Verlauf ist unregelmäßig, aber in einem regelmäßigen Muster. Zum Beispiel das Ausfallen eines Schlages alle "n" Schläge.</items>
          <items id="text">Normal unregelmäßig</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">Der Verlauf ist auf chaotische und unvorhersehbare Weise unregelmäßig. Zum Beispiel: Vorhofflimmern.</items>
          <items id="text">Ungleichmäßig unregelmäßig</items>
        </term_definitions>
        <term_definitions code="at0010">
          <items id="description">@ internal @</items>
          <items id="text">List</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="description">@ internal @</items>
          <items id="text">List</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">Die Körperhaltung des Patienten während der Untersuchung.</items>
          <items id="text">Körperhaltung</items>
        </term_definitions>
        <term_definitions code="at1000">
          <items id="description">Der Patient lag flach.</items>
          <items id="text">Liegend</items>
        </term_definitions>
        <term_definitions code="at1001">
          <items id="description">Der Patient saß (zum Beispiel auf einem Bett oder einem Stuhl).</items>
          <items id="text">Sitzend</items>
        </term_definitions>
        <term_definitions code="at1002">
          <items id="description">Der Patient saß zurück gelehnt in einem Winkel von 45 Grad und seine Beine waren bis zur Höhe des Beckens angehoben.</items>
          <items id="text">Zurücklehnend</items>
        </term_definitions>
        <term_definitions code="at1003">
          <items id="description">Der Patient stand, ging oder rannte.</items>
          <items id="text">Stehend/Aufrecht</items>
        </term_definitions>
        <term_definitions code="at1005">
          <items id="comment">Eine Puls-/Herzfrequenz ist vorhanden, wenn die Frequenz > 0 Schläge pro Minute ist.</items>
          <items id="description">Vorhandensein einer Puls- oder Herzfrequenz.</items>
          <items id="text">Vorhandensein</items>
        </term_definitions>
        <term_definitions code="at1013">
          <items id="description">Informationen zu dem Gerät, welches zur Messung der Puls- oder der Herzfrequenz verwendet wurde.</items>
          <items id="text">Gerät</items>
        </term_definitions>
        <term_definitions code="at1017">
          <items id="description">Details über die körperliche Anstrengung, die der Patient während der Untersuchung ausgesetzt war.</items>
          <items id="text">Anstrengung</items>
        </term_definitions>
        <term_definitions code="at1018">
          <items id="comment">Zum Beispiel das Vorhandensein eines Herzschrittmacher; das Angstniveau; Schmerzen oder Fieber etc.</items>
          <items id="description">Die Beschreibung aller zufälligen Faktoren, die die Interpretation der physikalischen Ergebnisse beeinflussen können.</items>
          <items id="text">Störfaktoren</items>
        </term_definitions>
        <term_definitions code="at1019">
          <items id="comment">Zum Beispiel auskultatorisch oder über elektronisches Monitoring, z.B. mit einem Blutdruckmessgerät.</items>
          <items id="description">Die Methode, mit der die Puls- oder Herzfrequenz gemessen wurde.</items>
          <items id="text">Methode</items>
        </term_definitions>
        <term_definitions code="at1022">
          <items id="description">Beschreibung der Puls- oder Herzfrequenz.</items>
          <items id="text">Klinische Beschreibung</items>
        </term_definitions>
        <term_definitions code="at1023">
          <items id="comment">Die Kodierung mit einer Terminologie wird, wenn möglich, bevorzugt. Zum Beispiel: Bradykardie, Extrasystolen oder Sinusrhythmus. Mehrere Aussagen sind erlaubt.</items>
          <items id="description">Ein einzelnes Wort, ein Satz oder eine kurze Beschreibung, welches die klinische Bedeutung und die Signifikanz der Puls- oder der Herzfrequenz, einschließlich des Rhythmus, darstellt.</items>
          <items id="text">Klinische Interpretation</items>
        </term_definitions>
        <term_definitions code="at1024">
          <items id="description">Eine Puls- oder eine Herzfrequenz kann ermittelt werden.</items>
          <items id="text">Vorhanden</items>
        </term_definitions>
        <term_definitions code="at1025">
          <items id="description">Eine Puls- oder eine Herzfrequenz kann nicht ermittelt werden.</items>
          <items id="text">Nicht ermittelbar</items>
        </term_definitions>
        <term_definitions code="at1028">
          <items id="description">Der Verlauf ist unregelmäßig.</items>
          <items id="text">Unregelmäßig</items>
        </term_definitions>
        <term_definitions code="at1030">
          <items id="comment">Die Kodierung mit einer Terminologie ist, wenn möglich, erwünscht. Zum Beispiel: kräftig, schwach, hämmernd, langsam ansteigend oder kollabierend. Mehrere Begriffe können dokumentiert werden.</items>
          <items id="description">Beschreibung des Merkmals der Puls- oder Herzfrequenz.</items>
          <items id="text">Merkmal</items>
        </term_definitions>
        <term_definitions code="at1032">
          <items id="description">Der Befund wurde durch physisches Tasten des Beobachters am Patienten erlangt.</items>
          <items id="text">Palpation</items>
        </term_definitions>
        <term_definitions code="at1033">
          <items id="description">Die Untersuchungsergebnisse werden mit Hilfe eines Gerätes, wie zum Beispiel einem Stethoskop, gemessen.</items>
          <items id="text">Auskultation</items>
        </term_definitions>
        <term_definitions code="at1034">
          <items id="description">Die Untersuchungsergebnisse werden nicht-invasiv unter Anwendung eines Gerätes, wie zum Beispiel mit Hilfe eines Pulsoximeters oder eines Stethoskops, gemessen.</items>
          <items id="text">Automatisch, nicht-invasiv</items>
        </term_definitions>
        <term_definitions code="at1036">
          <items id="description">Die maximale Puls- oder Herzfrequenz, die während einer Belastungsperiode gemessen wurde.</items>
          <items id="text">Maximum</items>
        </term_definitions>
        <term_definitions code="at1037">
          <items id="description">Die Körperstelle an der die Puls- oder die Herzfrequenz gemessen wird.</items>
          <items id="text">Körperstelle</items>
        </term_definitions>
        <term_definitions code="at1038">
          <items id="description">Die linke Arteria radialis.</items>
          <items id="text">Arteria radialis - links</items>
        </term_definitions>
        <term_definitions code="at1039">
          <items id="description">Die rechte Arteria radialis.</items>
          <items id="text">Arteria radialis - rechts</items>
        </term_definitions>
        <term_definitions code="at1040">
          <items id="description">Die Region des Herzens.</items>
          <items id="text">Herz</items>
        </term_definitions>
        <term_definitions code="at1041">
          <items id="description">Die linke Arteria carotis.</items>
          <items id="text">Arteria carotis - links</items>
        </term_definitions>
        <term_definitions code="at1042">
          <items id="description">Die rechte Arteria carotis.</items>
          <items id="text">Arteria carotis - rechts</items>
        </term_definitions>
        <term_definitions code="at1043">
          <items id="description">Die linke Arteria femoralis.</items>
          <items id="text">Arteria femoralis - links</items>
        </term_definitions>
        <term_definitions code="at1044">
          <items id="description">Die rechts Arteria femoralis.</items>
          <items id="text">Arteria femoralis - rechts</items>
        </term_definitions>
        <term_definitions code="at1047">
          <items id="description">Ein nicht näher beschriebener Finger.</items>
          <items id="text">Finger</items>
        </term_definitions>
        <term_definitions code="at1048">
          <items id="description">Die linke Arteria brachialis.</items>
          <items id="text">Arteria brachialis - links</items>
        </term_definitions>
        <term_definitions code="at1049">
          <items id="description">Die rechte Arteria brachialis.</items>
          <items id="text">Arteria brachialis - rechts</items>
        </term_definitions>
        <term_definitions code="at1050">
          <items id="description">Die Untersuchungsergebnisse werden invasiv durch die Verwendung eines Gerätes, wie zum Beispiel eines arteriellen Katheters, gemessen.</items>
          <items id="text">Automatisch, invasiv</items>
        </term_definitions>
        <term_definitions code="at1051">
          <items id="description">Das Ohrläppchen eines unbestimmten Ohres.</items>
          <items id="text">Ohrläppchen</items>
        </term_definitions>
        <term_definitions code="at1054">
          <items id="description">Ein nicht näher beschriebener Zeh.</items>
          <items id="text">Zeh</items>
        </term_definitions>
        <term_definitions code="at1055">
          <items id="comment">Die Auswahl eines Wertes dieser Wertemenge ist nur valide, wenn "Unregelmäßig" vom Datenelement "Regelmäßigkeit" angegeben wurde.</items>
          <items id="description">Ein spezifischeres Verlaufsmuster einer unregelmäßigen Puls- oder Herzfrequenz.</items>
          <items id="text">Unregelmäßiger Typ</items>
        </term_definitions>
        <term_definitions code="at1056">
          <items id="comment">Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen oder CIMI-Modelle.</items>
          <items id="description">Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.</items>
          <items id="text">Erweiterung</items>
        </term_definitions>
        <term_definitions code="at1057">
          <items id="description">Eine allgemeine Bezeichnung, um alle oder einen Teil der Aussagen über die Puls- oder die Herzfrequenz zu ermöglichen.</items>
          <items id="text">Klinische Interpretation</items>
        </term_definitions>
        <term_definitions code="at1058">
          <items id="description">Spezifische Schlussfolgerung über den Rhythmus der Puls- oder der Herzfrequenz, die sich aus einer Kombination von Herzfrequenz, dem Verlaufsmuster und anderen bei der Untersuchung beobachteten Merkmalen ergibt.</items>
          <items id="text">Rhythmus</items>
        </term_definitions>
        <term_definitions code="at1059">
          <items id="description">Zusätzliche Informationen über die Puls- oder die Herzfrequenz, die in anderen Bereichen nicht erfasst wurden.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
